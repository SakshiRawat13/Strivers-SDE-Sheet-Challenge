int findCelebrity(int n) {
 	stack <int> st;
	for(int i=0; i<n; i++){
		st.push(i);
	}
	while(st.size()>1){
		int a= st.top();
		st.pop();
		int b= st.top();
		st.pop();
		
		if(knows(a, b))
		st.push(b);
		else
		st.push(a);
	}
	if(st.empty())
	return -1;

	if(st.size()==1){
	for(int i=0; i<n; i++){
		if(st.top()==i)
		   continue;
		if(knows(st.top(), i) || !knows(i, st.top()))
			return -1;
	}
			return st.top();
	}
	return -1;
}
// tc: o(n), sc: o(n)
